{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "lib": [
            "ES7"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "outDir": "./build"
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended"
}
